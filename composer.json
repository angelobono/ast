{
  "name": "angelobono/ast",
  "version": "1.0.0",
  "type": "library",
  "require": {
    "php": "^8.1",
    "nikic/php-parser": "^4.17 || ^5.6"
  },
  "autoload": {
    "psr-4": {
      "Bono\\AST\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bono\\AST\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "infection/infection": "^0.28",
    "squizlabs/php_codesniffer": "^3.7",
    "maglnet/composer-require-checker": "^4.0",
    "vimeo/psalm": "^6.13"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
